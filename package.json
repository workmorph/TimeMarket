{
  "name": "time-bid",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,yml,yaml,css}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,yml,yaml,css}\"",
    "type-check": "tsc --noEmit",
    "pre-commit": "lint-staged",
    "prepare": "husky",
    "widget:dev": "vite --config vite.config.js",
    "widget:build": "tsc --project tsconfig.widget.json && vite build --config vite.config.js",
    "widget:preview": "vite preview --config vite.config.js",
    "widget:deploy": "ts-node scripts/deploy-widget.ts",
    "widget:deploy:patch": "ts-node scripts/deploy-widget.ts patch",
    "widget:deploy:minor": "ts-node scripts/deploy-widget.ts minor",
    "widget:deploy:major": "ts-node scripts/deploy-widget.ts major",
    "windsurf:scan": "ts-node scripts/windsurf-issue-scanner.ts",
    "windsurf:execute": "ts-node scripts/windsurf-executor.ts",
    "windsurf:status": "cat .windsurf/status-dashboard.md",
    "windsurf:log": "cat .windsurf/execution-log.md",
    "check-status": "bash check-status.sh",
    "test": "echo 'Tests to be implemented'",
    "test:api-keys": "NODE_DEBUG=* ts-node scripts/test-api-keys.ts",
    "lighthouse": "echo 'Lighthouse to be implemented'"
  },
